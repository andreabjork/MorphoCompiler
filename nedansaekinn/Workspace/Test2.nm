testANDORNOT() {
	var s, a, b;
	a = 1 - (2+4);
	b = 1 - 2 + 4;
	writeln("A is "++a);
	writeln("B is "++b);
	s = "";
	if(true && false || !false) {
		s = s ++ "This seems to be working correctly.";
	};

	if(false || true && !true) {
		s = s ++ "Definitely not working correctly";
	};

	return s;
}

main() {
	writeln("The output of the test is: " ++ testANDORNOT());
}
